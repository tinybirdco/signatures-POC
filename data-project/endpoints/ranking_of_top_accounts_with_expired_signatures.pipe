DESCRIPTION >
	Ranking of the top accounts having signatures expired:


TOKEN "ranking_of_top_accounts_with_expired_signatures_endpoint_read_4189" READ

TOKEN "read_all_pipes_token" READ

NODE ranking_of_top_accounts_with_expired_signatures_0
SQL >

    SELECT 
      account_id as name,
      count(id) as value 
    FROM 
      signatures 
    WHERE 
      status = 'expired' 
    GROUP BY 
      account_id 
    ORDER BY 
      value DESC
    LIMIT
      7


