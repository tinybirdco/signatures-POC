DESCRIPTION >
	Ranking of the top accounts creating signatures


TOKEN "ranking_of_top_accounts_creating_signatures_endpoint_read_4001" READ

TOKEN "read_all_pipes_token" READ

NODE ranking_of_top_accounts_creating_signatures_0
SQL >

    SELECT 
      account_id, 
      count(id) as total_signatures 
    FROM 
      signatures 
    GROUP BY 
      account_id 
    ORDER BY 
      total_signatures DESC
    LIMIT
      10


